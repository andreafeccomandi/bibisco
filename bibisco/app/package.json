    "zip-linux-se": "shx mv /home/test/work/release-builds/bibisco-linux-x64 /home/test/work/release-builds/bibisco-linux-x64-2.2.0-SE && cd /home/test/work/release-builds && bestzip bibisco-linux-x64-2.2.0-SE.zip bibisco-linux-x64-2.2.0-SE && shx rm -rf /home/test/work/release-builds/bibisco-linux-x64-2.2.0-SE",
