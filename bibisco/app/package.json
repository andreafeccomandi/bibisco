    "package-mac-ce": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=/Users/andreafeccomandi/work/release-builds --app-bundle-id=com.bibisco.2.0.0-CE --app-version=2.2.0.CE --build-version=1.0.100 --osx-sign.hardenedRuntime=true --osx-sign.type=distribution --osx-sign.entitlements=entitlements.mac.plist --osx-sign.entitlements-inherit=entitlements.mac.inherit.plist --osx-notarize.appleId='APPLE_ID' --osx-notarize.appleIdPassword=<APPLE_ID_PASSWORD> --ignore=/bibiscoenv --ignore=/@paulcbetts/cld/deps  --ignore=/@paulcbetts/cld/src --ignore=/@paulcbetts/cld/test --ignore=/@paulcbetts/spellchecker/src --ignore=/docx/ts --ignore=/example --ignore=/examples --ignore=/screenshots --ignore=/test --ignore=/tests",
    "package-mac-se": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=/Users/andreafeccomandi/work/release-builds --app-bundle-id=com.bibisco.2.0.0-SE --app-version=2.2.0.SE --build-version=1.0.100 --osx-sign.hardenedRuntime=true --osx-sign.type=distribution --osx-sign.entitlements=entitlements.mac.plist --osx-sign.entitlements-inherit=entitlements.mac.inherit.plist --osx-notarize.appleId='APPLE_ID' --osx-notarize.appleIdPassword=<APPLE_ID_PASSWORD> --ignore=/bibiscoenv --ignore=/@paulcbetts/cld/deps  --ignore=/@paulcbetts/cld/src --ignore=/@paulcbetts/cld/test --ignore=/@paulcbetts/spellchecker/src --ignore=/docx/ts --ignore=/example --ignore=/examples --ignore=/screenshots --ignore=/test --ignore=/tests",
